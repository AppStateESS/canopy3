{
  "require": {
    "doctrine/dbal": "^3.1",
    "oomphinc/composer-installers-extender": "^2.0",
    "appstateess/canopy3-theme-simple": "dev-master",
    "appstateess/canopy3-dashboard-setup": "^1.0",
    "appstateess/canopy3-dashboard-user": "^1.0",
    "ezyang/htmlpurifier": "^4.13"
  },
  "extra": {
    "installer-types": [
      "canopy3-theme",
      "canopy3-plugin",
      "canopy3-dashboard"
    ],
    "installer-paths": {
      "resources/themes/{$name}": [
        "type:canopy3-theme"
      ],
      "resources/plugins/{$name}": [
        "type:canopy3-plugin"
      ],
      "resources/dashboards/{$name}": [
        "type:canopy3-dashboard"
      ]
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/var/www/html/c3-themes/canopy3-theme-simple"
    },
    {
      "type": "path",
      "url": "/var/www/html/c3-dashboards/canopy3-dashboard-setup"
    },
    {
      "type": "path",
      "url": "/var/www/html/c3-dashboards/canopy3-dashboard-user"
    }
  ],
  "minimum-stability": "stable"
}
